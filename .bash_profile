alias source_bash='source ~/.bash_profile'

alias la='ls -la'
alias ll='ls -la'
alias hisgre='history | grep $1'

alias git_log='git log --oneline'
alias git_log_detail='git log'
alias git_commit='git commit -v'
alias git_push='git push'
alias git_branch='git branch -a'
alias git_checkout='git checkout $1'
alias git_diff='git diff --name-only'
alias git_diff_detail='git diff'
alias git_reset='git reset --hard HEAD'

alias pod_init='pod init'
alias pod_install='pod install'
alias pod_update='pod update'
